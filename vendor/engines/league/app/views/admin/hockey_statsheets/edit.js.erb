$('#dialog-modal').html("<%= escape_javascript( render(:partial => @partial ) )%>");
$('#dialog-modal').dialog({
  title: '<%= @title %>',
  height: 400,
  width: 500,
  modal: true,
  buttons: {
    Save: function() {
      $(this).children("form").submit();
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
});
