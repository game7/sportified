- title "Divisions"

%table
  %tr
    %th Name
    %th Actions
  - @divisions.each do |division|
    %tr
      %td #{division.name}
      %td
        = link_to 'Go To', [:admin, division]
        = link_to 'Edit', edit_admin_division_path(division)
        = link_to 'Destroy', [:admin, division], :confirm => 'Are you sure?', :method => :delete
%br/
= link_to 'New Division', new_admin_division_path
