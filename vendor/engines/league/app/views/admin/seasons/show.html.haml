- title @season.name
%table.table.table-striped.table-bordered
  %thead
    %tr
      %th Name
      %th Actions
    %tbody
    - @divisions.each do |division|
      %tr
        %td #{division.name}
        %td
          = link_to 'Edit', edit_admin_division_path(division), :class => "btn btn-mini"  
          = link_to 'Delete', [:admin, division], :confirm => 'Are you sure?', :method => :delete, :class => "btn btn-mini"  
= link_to 'New Division', new_admin_season_division_path(@season), :class => "btn"



