- title "Clubs"

%table.table.table-striped.table-bordered
  %thead
    %tr
      %th Name
      %th Actions
  %tbody
    - @clubs.each do |club|
      %tr
        %td #{club.name}
        %td
          = link_to 'Edit', edit_admin_club_path(club), :class => 'btn btn-mini'
          = link_to 'Destroy', [:admin, club], :confirm => 'Are you sure?', :method => :delete, :class => 'btn btn-mini'
= link_to 'New Club', new_admin_club_path, :class => "btn"
