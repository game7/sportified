- require 'csv'

= semantic_form_for [:admin, @game_upload], :html => {:multipart => true} do |f|
  = f.inputs :name => "Teams" do
    = f.semantic_fields_for :team_maps do |t|
      = t.input :team_id, :as => :select, :collection => @team_options, :label => t.object.name
  = f.inputs :name => "Venues" do
    = f.semantic_fields_for :venue_maps do |t|
      = t.input :venue_id, :as => :select, :collection => @venue_options, :label => t.object.name
  = f.buttons do
    = f.commit_button
    %li #{ link_to 'Cancel', admin_game_uploads_path }

.data
  %table
    - if Rails.env.production?
      - rows = CSV.read(@game_upload.file.url)
    - else
      - rows = CSV.read(@game_upload.file.path)
    - rows.each do |row|
      %tr
        - row.each do |col|
          %td #{col}



    
