- title "Game Uploads"

%table
  %tr
    %th Date
    %th Season
    %th Ready
    %th Processed
    %th Actions
  - @game_uploads.each do |upload|
    %tr
      %td #{upload.created_at}
      %td #{upload.season_name}
      %td #{upload.ready}
      %td #{upload.completed}
      %td
        = link_to 'Edit', edit_admin_game_upload_path(upload) unless upload.completed
        = link_to 'Complete', complete_admin_game_upload_path(upload), :method => :post if upload.ready && !upload.completed
        /= link_to 'Destroy', [:admin, club], :confirm => 'Are you sure?', :method => :delete
= link_to 'New Game Upload', new_admin_game_upload_path
