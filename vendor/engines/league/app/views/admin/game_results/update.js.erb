<% if @result.errors.any? %>
  $("#flash_notice").hide(300);
  $('#dialog-modal').html("<%= escape_javascript( render(:partial => "admin/game_results/edit", :object => @result ) )%>");
<% else %>
  $("#flash_notice").html("<%= escape_javascript( flash.delete(:notice) )%>");
  $("#flash_notice").show(300);
  $("#game_<%= @result.id %>").replaceWith("<%= escape_javascript( render(:partial => "admin/games/game", :object => @result.game ) )%>");
  $('#dialog-modal').dialog("close");
<% end %>
