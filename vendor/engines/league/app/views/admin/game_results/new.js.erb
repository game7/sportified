$('#dialog-modal').html("<%= escape_javascript( render(:partial => "admin/game_results/new", :object => @result ) )%>");
$('#dialog-modal').dialog({
  title: 'Post Game Result',
  height: 300,
  width: 400,
  modal: true,
  buttons: {
    Save: function() {
      $(this).children("form").submit();
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
});
