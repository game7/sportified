$('#dialog-modal').html("<%= escape_javascript( render(:partial => "text_blocks/form" ) )%>");
$(".wymeditor").wymeditor();
$('#dialog-modal').dialog({
  title: 'Edit Text Block',
  height: 600,
  width: 800,
  modal: true,
  buttons: {
    Save: function() {
      $.each(WYMeditor.INSTANCES, function() {
        this.update();
      });
      WYMeditor.INSTANCES.length = 0;
      $(this).children("form").submit();
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
}); 
