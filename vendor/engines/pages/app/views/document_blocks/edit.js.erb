$('#dialog-modal').html("<%= escape_javascript( render(:partial => "document_blocks/form" ) )%>");
$('#dialog-modal').dialog({
  title: 'Edit Document ',
  height: 500,
  width: 600,
  modal: true,
  buttons: {
    Save: function() {
      $(this).children("form").submit();
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
}); 
