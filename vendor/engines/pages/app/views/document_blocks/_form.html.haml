= semantic_form_for [@page, @block], :html => {:target => 'frame', :multipart => true} do |f|
  = f.inputs do
    = f.input :title
    - if @block.document?
      %li #{@block.document.url}
    = f.input :document, :as => :file
%iframe{ :id => 'frame', :name => 'frame', :style => 'display: none;' }

