$('#dialog-modal').html("<%= escape_javascript( render(:partial => "admin/hockey_goaltenders/edit" ) )%>");
$('#dialog-modal').dialog({
  title: 'Edit Goaltender',
  height: 440,
  width: 500,
  modal: true,
  buttons: {
    Save: function() {
      $(this).children("form").submit();
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
});
