<% if @chromecast.errors.any? %>
  var form = '<%= j(render('form')) %>';
  $('#edit_chromecast_<%= @chromecast.id %>').replaceWith(form);
  window.sportified.chromecast.initialize();
<% else %>
  window.sportified.chromecast.setChromecastId('<%= @chromecast.id %>');
  window.location = '<%= admin_chromecasts_path %>';
<% end %>
