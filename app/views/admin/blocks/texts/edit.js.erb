$('#dialog-modal').html("<%= escape_javascript( render(:partial => "admin/blocks/texts/form" ) )%>");
$('#dialog-modal').dialog({
  title: 'Edit Text',
  height: 500,
  width: 800,
  modal: true,
  buttons: {
    Save: function() {
      $(this).children("form").submit();      
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
}); 

