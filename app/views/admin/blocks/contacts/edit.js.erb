$('#dialog-modal').html("<%= escape_javascript( render(:partial => "admin/blocks/contacts/form" ) )%>");
$('#dialog-modal').dialog({
  title: 'Edit Contact',
  height: 375,
  width: 500,
  modal: true,
  buttons: {
    Save: function() {
      $(this).children("form").submit();
    },
    Cancel: function() {
      $(this).dialog("close");
    }
  }
}); 
